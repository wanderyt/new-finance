{
  "name": "new-finance",
  "version": "0.12.0",
  "private": true,
  "packageManager": "yarn@4.5.3",
  "engines": {
    "node": ">=20.9.0"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "db:studio": "drizzle-kit studio",
    "db:push": "drizzle-kit push",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx scripts/seed.ts",
    "db:cleanup-schedules": "tsx scripts/cleanup-schedule-rules.ts",
    "db:sync-to-main": "cp db/finance.db ../new-finance/db/finance.db && echo 'Database synced to main branch'",
    "db:sync-from-main": "mkdir -p db && cp ../new-finance/db/finance.db db/finance.db && echo 'Database synced from main branch'",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@reduxjs/toolkit": "^2.11.2",
    "axios": "^1.13.2",
    "drizzle-orm": "0.45.1",
    "framer-motion": "^12.23.26",
    "next": "16.1.0",
    "openai": "^6.15.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "better-sqlite3": "^12.5.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "eslint-config-next": "16.1.0",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}
